#include <iostream>
#include <cstdlib>
using namespace std;

int main() {
	
	int n, d, arr[n], jolly=0;
	bool arr2[n] = {false};
	while(n!=0){
	    
	    cin>>n;
        for(int i=0; i<n; i++)
    	{
    	    cin>>arr[i];
    	}
    	for(int i=0; i<n-1; i++)
    	{
    	    d= arr[i+1] - arr[i];
    	    
    	    
    	    if(abs(d)>=1 && abs(d)<n-1)
    	    {
    	        arr2[abs(d)] = true;
    	    }
    	}
    	for(int i=1; i<n-1; i++)
    	{
    	    if(arr2[i]== false)
    	    {
    	        jolly=1;
    	        break;
    	    }
    	}
    	if(jolly==1)
    	    cout<<"Not jolly";
    	else
    	    cout<<"Jolly";
    	
    	return 0;
	}
}
